{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "docker:build": "docker compose --env-file .env build",
    "docker:up": "docker compose --env-file .env up --build",
    "docker:down": "docker compose --env-file .env down",
    "docker:logs": "docker compose --env-file .env logs -f",
    "docker:dev:build": "docker compose --env-file .env -f docker-compose.dev.yml build",
    "docker:dev:up": "docker compose --env-file .env -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker compose --env-file .env -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker compose --env-file .env -f docker-compose.dev.yml logs -f",
    "format": "turbo run format",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.7.5"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@10.10.0"
}
